#include<stdio.h>
/**
 *main - "Size of various types"
 *Return: 0
 */
int main(void)
{
  int intType;
  float floatType;
  double doubleType;
  char charType;
  long longType;
  long long longLongType;

  printf("Size of a char: %zu byte(s)\n", sizeof(charType));
  printf("Size of an int: %zu byte(s)\n", sizeof(intType))
  
  return (0);
}
