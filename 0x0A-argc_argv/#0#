
	if (argc < 2)
	   {
			printf("0\n");
					return (0);
					       }
						while (--argc)
						      {
								if (isInteger(argv[argc]))
									{
												printf("Error\n");
															return (1);
																}
																		sum += atoi(argv[argc]);
																		    }
																			printf("%d\n", sum);
																				       exit(EXIT_SUCCESS);
